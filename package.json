{
  "name": "react-sliding-container-demo",
  "version": "0.0.1",
  "description": "Demo of react-sliding-container",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass"
  ],
  "author": "Vygandas Pliasas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vygandas/react-sliding-container-demo"
  },
  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:fix": "tslint './src/**/*.ts*' --format stylish --force --fix && stylelint ./src/**/*.scss --fix",
    "lint:ts": "tslint './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "npm run build && node express.js",
    "test": "jest --coverage --config=configs/jest.json",
    "test:watch": "jest --watch --coverage --config=configs/jest.json",
    "deploy": "jest --coverage --config=configs/jest.json && npm run build"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.1",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "esnext": "^3.3.1",
    "expect": "^23.2.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.0",
    "jest": "^23.1.0",
    "jest-enzyme": "^6.0.2",
    "node-sass": "^4.9.0",
    "postcss-loader": "^2.1.5",
    "query-string": "^6.1.0",
    "react": "^16.4.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^4.3.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "unfetch": "^3.0.0",
    "webpack": "^4.11.1",
    "webpack-cli": "^3.0.3",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "react-sliding-container": "0.0.8"
  },
  "postcss": {},
  "moduleDirectories": [
    "node_modules",
    "src"
  ]
}
